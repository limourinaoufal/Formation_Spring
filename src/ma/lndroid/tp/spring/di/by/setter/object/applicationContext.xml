<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="employeeBean" class="ma.lndroid.tp.spring.di.by.setter.object.Employee">
		<property name="id" value="22" />
		<property name="name" value="LINA" />
		<property name="address" ref="addressBean" />
	</bean>

	<bean id="addressBean" class="ma.lndroid.tp.spring.di.by.setter.object.Address">
		<property name="city" value="Rabat" />
		<property name="state" value="Rabat/Sale" />
		<property name="country" value="Morocco" />
	</bean>

</beans>  