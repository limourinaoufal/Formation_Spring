<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="questionBean"
		class="ma.lndroid.tp.spring.di.by.setter.map.with.primitive.Question">
		<property name="id" value="100"/>
		<property name="name" value="What is the modules of Spring ?"/>
		<property name="answers" >
		<map>
			<entry key="1" value="AOP"/>
				<entry key="2" value="DATA"/>
				<entry key="3" value="SECURITY ..."/>
		</map>
		</property>

	</bean>

	
</beans>  